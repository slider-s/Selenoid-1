<suite name="Parallel Tests" verbose="1" thread-count="4" parallel="tests">
    <tests>
        <test name="Chrome 63 Test">
            <parameters>
                <parameter name="browserName" value="chrome" />
                <parameter name="browserVersion" value="65"/>
            </parameters>
            <classes>
                <class name="p900.p900Test" />
                <class name="p900.promenuTest" />
            </classes>
        </test>
        <test name="Firefox 57 Test">
            <parameters>
                <parameter name="browserName" value="firefox" />
                <parameter name="browserVersion" value="59"/>
            </parameters>
            <classes>
                <class name="p900.p900Test" />
                <class name="p900.promenuTest" />
            </classes>
        </test>
    </tests>
</suite>